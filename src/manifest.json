{
    "name": "Craigslist Preview",
    "version": "1.0.11",
    "description": "Extension to prefetch Craigslist ad images",
    "background_page": "background.html",
    "icons": { "48": "icon48.png",
    	       "128": "icon128.png" },
    "permissions": [
	"http://*.craigslist.org/",
	"http://*.craigslist.ca/"
    ],
    "content_scripts": [
	{
	    "matches": [
	        "http://*.craigslist.org/*",
		"http://*.craigslist.ca/*"
	    ],
	    "js": [
		"jquery-1.3.2.min.js", 
		"jquery-ui-1.7.2.custom.min.js", 
		"jquery.ba-dotimeout.js", 
		"xhrproxy.js", 
		"settingsProxy.js", 
		"cl.js"],
	    "css": ["cl.css"],
	    "run_at": "document_start"
	}
    ],
    "options_page": "options.html"
}
